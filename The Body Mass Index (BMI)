from math import trunc

length = input("Enter length in meters: ")
length = float(length)
weigth = input("Enter your weight in kilograms: ")
weigth = float(weigth)
bmi = (trunc((weigth/(length*length))*100)/100)
print(bmi)

if (bmi < 18.5):
    print("Underweigth")
elif ((bmi >= 18.5) and (bmi <= 24.9)):
    print("Normal Weigth")
elif ((bmi >= 25) and (bmi <= 29.9)):
    print("Overweigth")
else:
    print("Obese")
